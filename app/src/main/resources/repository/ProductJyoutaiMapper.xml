<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sotugyouseisaku.app.repository.view.ProductJyoutaiMapper">

    <!-- product_view から状態を重複除去して取得 -->
    <select id="selectDistinctJyoutai" resultType="com.sotugyouseisaku.app.Record.ProductJyoutaiRecord">
        SELECT DISTINCT jyoutai
        FROM online_shop.product_view
        ORDER BY jyoutai
    </select>

</mapper>
